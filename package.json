  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.12",
    "nodemon": "^2.0.7",
    "socket.io": "^4.1.2",
    "sequelize": "^6.6.2"

  }
